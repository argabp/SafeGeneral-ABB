@model ABB.Web.Modules.MutasiKlaim.Models.MutasiKlaimObyekViewModel
@using Kendo.Mvc.UI
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Kendo.Mvc
@addTagHelper *, ABB.Web

<script src="~/Modules/MutasiKlaim/JS/mutasiKlaim.obyek.js"></script>

<div class="window-body" style="background-color: lightgray">
    <form id="MutasiKlaimObyekForm" enctype="multipart/form-data" autocomplete="off">
        @Html.HiddenFor(h => h.kd_cb)
        @Html.HiddenFor(h => h.kd_cob)
        @Html.HiddenFor(h => h.kd_scob)
        @Html.HiddenFor(h => h.kd_thn)
        @Html.HiddenFor(h => h.no_kl)
        @Html.HiddenFor(h => h.no_mts)
        <div class="form-group row">
            <div class="row col-sm-12">
                <label class="col-sm-4">Nomor Obyek</label>
                <div class="col-sm-8">
                    <kendo-textbox for="no_oby" readonly="true" style="width: 100%"/>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="row col-sm-12">
                <label class="col-sm-4">Nama Obyek</label>
                <div class="col-sm-8">
                    <kendo-textbox for="nm_oby" readonly="true" style="width: 100%"/>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="row col-sm-12">
                <label class="col-sm-4">Nilai Pertanggungan</label>
                <div class="col-sm-8">
                    <kendo-numerictextbox decimals="2" spinners="false" for="nilai_ttl_ptg" readonly="true" style="width: 100%;"/>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="row col-sm-12">
                <label class="col-sm-4">Nilai Klaim</label>
                <div class="col-sm-8">
                    <kendo-numerictextbox decimals="2" spinners="false" for="nilai_kl" readonly="true" style="width: 100%;"/>
                </div>
            </div>
        </div>
    </form>
</div>