@model ABB.Web.Modules.COA.Models.CoaViewModel
@using Kendo.Mvc.UI

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Kendo.Mvc

<div class="window-body" style="background-color: lightgray">
    <form id="CoaForm" enctype="multipart/form-data" autocomplete="off">
        <div class="row">
            <div class="form-group col-sm-8">
                    <label asp-for="Kode"></label>
                    <div>
                        <kendo-textbox for="Kode" style="width: 100%;" />
                        <span asp-validation-for="Kode" class="text-danger"></span>
                    </div>
            </div>
        </div>

         <div class="row">
            <div class="form-group col-sm-8">
                    <label asp-for="Nama"></label>
                    <div>
                        <kendo-textbox for="Nama" style="width: 100%;" />
                        <span asp-validation-for="Nama" class="text-danger"></span>
                    </div>
            </div>
        </div>

         <div class="row">
            <div class="form-group col-sm-8">
                    <label asp-for="Dept"></label>
                    <div>
                        <kendo-textbox for="Dept" style="width: 100%;" maxlength="2"/>
                        <span asp-validation-for="Dept" class="text-danger"></span>
                    </div>
            </div>
        </div>

         <div class="row">
            <div class="form-group col-sm-8">
                    <label asp-for="Type"></label>
                    <div>
                        <kendo-textbox for="Type" style="width: 100%;" maxlength="2" />
                        <span asp-validation-for="Type" class="text-danger"></span>
                    </div>
            </div>
        </div>

       
        
    </form>
</div>

<div class="window-footer">
    <button type="button" onclick="onSaveCoa()" class="btn btn-success space-right">
        <span class="fa fa-save"></span> Save
    </button>
</div>