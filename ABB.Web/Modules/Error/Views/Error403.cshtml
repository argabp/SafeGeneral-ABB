@using Kendo.Mvc.UI
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Kendo.Mvc
@{
    Layout = "~/Modules/Shared/_Layout.cshtml";
    ViewData["Title"] = "Access Denied Page";
}

<div class="flat-box">

    <div class="error-page">
        <h2 class="headline text-warning" style="margin-right:20px"> 403</h2>

        <div class="error-content" style="margin-top: 20px">
            <h3><i class="fas fa-exclamation-triangle text-warning"></i> Oops! Access Denied!</h3>

            <p>
                You don't have access to this page
                <a href="/home/index">return to dashboard</a>
            </p>


        </div>
        <!-- /.error-content -->
    </div>

</div>