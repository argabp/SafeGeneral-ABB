@using Kendo.Mvc.UI

@(Html.Kendo().DropDownList()
    .Name("kd_prop_dt") // The name of the component has to be the same as the name of the property.
    .DataValueField("Value") // The value of the drop-down is taken from the EmployeeID property.
    .DataTextField("Text") // The text of the items is taken from the EmployeeName property.
    .BindTo((System.Collections.IEnumerable)ViewData["provinsi"]) // A list of all employees which is populated in the controller.
    )